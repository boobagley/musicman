<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
  <category>
    <pattern>
      Which album is <set>songs</set>^
    </pattern>
    <template>
      <think>
        <set name="album">
          <map name="songalbum">
            <star/>
          </map>
        </set>
      </think>
      <condition name="album">
        <li value="unknown">
          <star/> is a single, it's not in an album.
        </li>
        <li>
          <star/> is from the album: <get name="album"/>
        </li>
      </condition>
    </template>
  </category>

  <category>
    <pattern>
      Do you know which album <set>songs</set>^
    </pattern>
    <template>
      <think>
        <set name="album">
          <map name="songalbum">
            <star/>
          </map>
        </set>
      </think>
      <condition name="album">
        <li value="unknown">
          <star/> is a single, it's not in an album.
        </li>
        <li>
          <star/> is from the album: <get name="album"/>
        </li>
      </condition>
    </template>
  </category>

  <category>
    <pattern>
      What year was <set>albums</set>^
    </pattern>
    <template>
      <think>
        <set name="year">
          <map name="albumyear">
            <star/>
          </map>
        </set>
      </think>
      <condition name="year">
        <li value="unknown">
          I'm not sure when <star/> was released
        </li>
        <li>
          <star/> was released in <get name="year"/>
        </li>
      </condition>
    </template>
  </category>
</aiml>